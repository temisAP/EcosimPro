EXPERIMENT military ON turbofan_mixer.transient
   DECLS
      TABLE_1D FuelFlow = {
            {0., 1., 2. , 12. , 14., 20.},
            {1., 1., 0.2,  0.2,  1.,  1.}
         }

   INIT
      -- Dynamic variables
      compressor.N = 12000.
      fan.N = 4000.

      -- Algebraic variables
      compressor.beta = 0.7

      fan.BPR = 1.
      fan.beta = 0.7
      fan.g_in.W = 50.

      hp_turbine.g_out.T = 1500.

      mixer.Ps1 = 3.5e+5
      mixer.g_in1.T = 1260.

   BOUNDS
      TURBOJET.Altitude = 0.
      TURBOJET.Mach = 0.

      fuel_valve.s_flow.signal[1] = 0.7679665 * timeTableInterp(TIME, FuelFlow)

   BODY
      REPORT_TABLE("reportAll", " * ")

      ABS_ERROR = 5.e-4
