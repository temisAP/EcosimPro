      fan.CG4 = 560.0476884
      fan.ND = 4350.465819

      hp_turbine.CG1 = 2.627911806
      hp_turbine.CG2 = 0.92
      hp_turbine.CG3 = 282.8427125
      hp_turbine.CG4 = 213.437858

      lp_turbine.CG1 = 5.96117921
      lp_turbine.CG2 = 0.92
      lp_turbine.CG3 = 103.8628134
      lp_turbine.CG4 = 193.5334004

      nozzle.A_exit = 0.09001966522

      mixer.A1 = 0.06016715927
      mixer.A2 = 0.04729949907

      STEADY()

      TSTOP = 30.
      CINT = 0.25
      INTEG()

END EXPERIMENT
