		PRINT("	LPT pressure ratio = $pi_LPT")
		PRINT("	HPT temperature ratio = $HPT.alpha")
		PRINT("	LPT temperature ratio = $LPT.alpha")
		PRINT("	Nozzle outlet pressure = $Nozzle.p_out")		
		PRINT("	Ambient pressure = $Nozzle.p_amb")
		
