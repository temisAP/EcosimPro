EXPERIMENT exp1 ON TurbojetEngine_design.default
   DECLS
   OBJECTS
   INIT
      -- initial values for algebraics
      HPC.M_in = 0.001
      LPC.M_in = 0.001

   BOUNDS
      -- Set equations for boundaries: boundVar = f(TIME;...)
